<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="EmploiRepere\Entity\Db\EmploiRepereCodeFonctionFicheMetier" table="emploirepere_emploirepere_codefonction_fichemetier">

        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>

        <many-to-one target-entity="EmploiRepere\Entity\Db\EmploiRepere"  field="emploiRepere">
            <join-column name="emploirepere_id" />
        </many-to-one>

        <many-to-one target-entity="FicheMetier\Entity\Db\CodeFonction"  field="codeFonction">
            <join-column name="codefonction_id" />
        </many-to-one>

        <many-to-one target-entity="FicheMetier\Entity\Db\FicheMetier"  field="ficheMetier">
            <join-column name="fichemetier_id" />
        </many-to-one>

    </entity>
</doctrine-mapping>
