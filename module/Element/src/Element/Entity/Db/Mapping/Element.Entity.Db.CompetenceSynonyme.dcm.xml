<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping https://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Element\Entity\Db\CompetenceSynonyme" table="element_competence_synonyme">

        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>

        <many-to-one target-entity="Element\Entity\Db\Competence" field="competence">
            <join-column name="competence_id" referenced-column-name="id"/>
        </many-to-one>
        <field name="libelle" length="255" column="libelle"/>

    </entity>
</doctrine-mapping>
