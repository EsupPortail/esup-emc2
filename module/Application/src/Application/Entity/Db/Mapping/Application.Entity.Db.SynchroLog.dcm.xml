<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Application\Entity\Db\SynchroLog" table="synchro_log">

        <id name="id" type="integer" column="id">
            <generator strategy="IDENTITY"/>
        </id>

        <many-to-one target-entity="Application\Entity\Db\SynchroJob" field="job">
            <join-column name="synchro_job" referenced-column-name="id" />
        </many-to-one>

        <field name="date"        type="datetime"                  column="date"                   nullable="true"/>
        <field name="rapport"     type="string" length="4096"      column="rapport"                nullable="true"/>

    </entity>
</doctrine-mapping>
