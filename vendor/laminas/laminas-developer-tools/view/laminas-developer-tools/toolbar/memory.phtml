<div class="laminas-toolbar-entry">
    <div class="laminas-toolbar-preview">
        <img src="data:image/jpeg;base64," alt="Memory Peak">
        <span class="laminas-toolbar-info">
            <?php echo $this->LaminasDeveloperToolsMemory($this->collector->getMemory()); ?>
        </span>
    </div>
    <div class="laminas-toolbar-detail<?php echo ($this->collector->hasEventMemory()) ? ' laminas-toolbar-detail-overflow' : ' laminas-toolbar-detail-redundant' ;?>">
        <?php if ($this->collector->hasEventMemory()): ?>
            <?php foreach ($this->collector->getApplicationEventMemory() as $key => $value): ?>
                <span class="laminas-toolbar-info">
                <span class="laminas-detail-label"><?php printf('%d. %s', $key + 1, $value['name']); ?></span>
                <span class="laminas-detail-value laminas-detail-value-right"><?php echo $this->LaminasDeveloperToolsMemory($value['difference']); ?></span>
                <p class="laminas-detail-context"><?php printf('%s - Line: %d', $value['file'], $value['line']); ?></p>
                </span>
            <?php endforeach; ?>
        <?php endif; ?>
        <span class="laminas-toolbar-info laminas-toolbar-info-redundant<?php echo ($this->collector->hasEventMemory()) ? ' laminas-toolbar-topspacing' : '' ;?>">
            <span class="laminas-detail-label">Memory Peak</span>
            <span class="laminas-detail-value laminas-detail-value-right">
                <?php echo $this->LaminasDeveloperToolsMemory($this->collector->getMemory()); ?>
            </span>
        </span>
    </div>
</div>